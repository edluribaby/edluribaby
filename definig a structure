#include<stdio.h>
#include<conio.h>
struct person
    {
        int age;
        char name[10]
    };
void main()
    {
        struct person person1;
        struct person person2;
        printf("enter the name\n");
        scanf("%s",person1.name);
        printf("enter the age\n");
        scanf("%d",&person2.age);
        printf("%s",person1.name);
        printf("%d",person2.age);
        
    }

